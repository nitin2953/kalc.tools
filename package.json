{
  "name": "kalc.tools",
  "version": "1.0.0",
  "description": "Online Calculators, Converters & Tools packed in a beautiful and easy to use website",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rimraf dist",
    "11ty": "npx @11ty/eleventy",
    "11ty:build": "npx @11ty/eleventy",
    "11ty:watch": "npx @11ty/eleventy --watch --incremental",
    "gulp:watch": "gulp watch",
    "gulp:serve": "gulp serve",
    "gulp:build": "gulp build",
    "watch": "npm-run-all --parallel 11ty:watch gulp:watch",
    "serve": "npm-run-all --parallel 11ty:watch gulp:serve",
    "test": "npm-run-all clean gulp:build 11ty:build",
    "build": "cross-env NODE_ENV=production npm-run-all clean gulp:build 11ty:build"
  },
  "author": "नितिन कुमार",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.5",
    "dev-ip": "^1.0.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^3.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-terser": "^2.1.0",
    "html-minifier-terser": "^7.0.0-alpha.2",
    "markdown-it": "^12.3.2",
    "markdown-it-attrs": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "qrcode-terminal": "^0.12.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.10"
  }
}
